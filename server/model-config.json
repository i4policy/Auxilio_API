{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../node_modules/loopback-ds-timestamp-mixin",
      "../node_modules/loopback-softdelete-2-mixin",
      "../node_modules/loopback-row-count-mixin",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ACL": {
    "dataSource": "mongo",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongo",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "CustomAccessToken": {
    "dataSource": "mongo",
    "public": false
  },
  "UserAccount": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "logout": true,
          "find":true,
          "findById":true,
          "deleteById": true,
          "replaceById":true,
          "patchAttributes":true,
          "registerMember": true,
          "appResetPassword": true,
          "appVerifyResetPassword": true,
          "updatePassword": true
        }
      }
    }
  },
  "UserRole": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find":true,
          "findById":true
        }
      }
    }
  },
  "PostCategory": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find":true,
          "findById":true,
          "create":true,
          "deleteById": true,
          "replaceById":true,
          "patchAttributes":true
        }
      }
    }
  },
  "Tag": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "create": true,
          "deleteById": true
        }
      }
    }
  },
  "Post": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find":true,
          "findById":true,
          "create":true,
          "deleteById": true,
          "replaceById":true,
          "patchAttributes":true,
          "updateMyPost": true,
          "deleteMyPost": true,
          "createPost": true
        }
      }
    }
  },
  "Feedback": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find":true,
          "findById":true,
          "create":true,
          "deleteById": true,
          "replaceById":true,
          "patchAttributes":true,
          "updateMyFeedback": true,
          "deleteMyFeedback": true,
          "createFeedback": true
        }
      }
    }
  },
  "FeedbackReplay": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find":true,
          "findById":true,
          "create":true,
          "deleteById": true,
          "replaceById":true,
          "patchAttributes":true,
          "updateMyReply": true,
          "deleteMyReply": true,
          "createReply": true
        }
      }
    }
  },
  "Email": {
    "dataSource": "email"
  },
  "PasswordReset": {
    "dataSource": "mongo",
    "public": false,
    "remoting": {
      "sharedMethods": {
        "*": false
      }
    }
  },
  "Container": {
    "dataSource": "storage",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "download": true,
          "customUpload": true
        }
      }
    }
  }
}
